<Window x:Class="Notes_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notes_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="15">
            <Button Width="80" Height="50" Content="Notatki" Click="OpenNotesWindow_Click"/>
            <Button Width="80" Height="50" Content="Wydarzenia" Click="OpenEventsWindow_Click"/>
            <Button Width="80" Height="50" Content="Zaloguj" Click="OpenLoginWindow_Click"/>
        </StackPanel>
    </Grid>
</Window>



