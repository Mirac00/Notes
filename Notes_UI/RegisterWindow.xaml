<Window x:Class="Notes_UI.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notes_UI"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Nazwa użytkownika:"/>
            <TextBox x:Name="UserNameTextBox"/>
            <Label Content="Hasło:"/>
            <PasswordBox x:Name="PasswordBox"/>
            <Button Content="Załóż konto" Click="RegisterButton_Click"/>
            <Button Content="Wróć" Click="BackButton_Click"/>
            <Label x:Name="StatusLabel" Content=""/>
        </StackPanel>
    </Grid>
</Window>
